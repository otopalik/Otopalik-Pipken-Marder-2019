g = new Graph()
g.size(0, 12000, -60, -45)
g.addvar("cable1.v(0.05)",1,1,0.6,0.9,2)
g.addvar("cable2.v(0.05)",2,1,0.6,0.9,2)
g.addvar("cable3.v(0.05)",3,1,0.6,0.9,2)
g.addvar("cable4.v(0.05)",4,1,0.6,0.9,2)
g.addvar("cable5.v(0.05)",5,1,0.6,0.9,2)
g.addvar("cable6.v(0.05)",6,1,0.6,0.9,2)




// SIMULATION CONTROL //
dt = 0.1// msec
tstop = 12000 //duration of simulation
v_init = -50 // V rest
proc initialize() {
	finitialize(v_init)
	fcurrent()
}

proc integrate() {
	g.begin()
	while(t<tstop) {
		fadvance()
		g.plot(t)
	}
	g.flush()
}

proc go() {
	initialize()
	integrate()
}


